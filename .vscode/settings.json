{
  "java.server.launchMode": "Standard",
  "java.project.sourcePaths": ["javabot", "example_bots/java"],
  "java.project.referencedLibraries": [
    "lib/**/*.jar",
    "javabot/libs/jackson-annotations-2.15.2.jar",
    "javabot/libs/jackson-core-2.15.2.jar",
    "javabot/libs/jackson-databind-2.9.9.3.jar"
  ],
  "debug.showInStatusBar": "never",
  "taskExplorer.enabledTasks": {
    "ant": false,
    "appPublisher": false,
    "bash": false,
    "composer": false,
    "gradle": false,
    "grunt": false,
    "gulp": false,
    "make": false,
    "maven": false,
    "npm": false,
    "nsis": false,
    "perl": false,
    "pipenv": false,
    "powershell": false,
    "python": false,
    "ruby": false,
    "tsc": false,
    "workspace": true
  },
  "taskExplorer.showLastTasks": false,
  "workbench.statusBar.visible": false,
  "taskExplorer.expanded": {
    "userTasks": true
  },
  "breadcrumbs.enabled": false,
  "workbench.activityBar.location": "hidden",
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.devcontainer": true,
    "**/.vscode": true,
    "req1.txt": true,
    "req2.txt": true,
    "requirements.txt": true,
    "TestObs.json": true,
    "test.py": true,
    "javabot/libs": true,
    "javabot/java_wrapper.py": true,
    "javabot/Observable.java": true,
    "javabot/PokerUtilities.java": true,
    "javabot/Range.java": true,
    "javabot/SuperBot.java": true,
    "main.py": true
  },
  "extensions.ignoreRecommendations": true,
  "window.commandCenter": false,
  "window.title": "${dirty}${activeEditorShort}${separator}",
  "workbench.layoutControl.enabled": false,

}